<h2><%=@committee.name%></h2>
  <%@committee.members.each do | member |%>
    <p>
      <ul class="contact">
        <li><%= member.name %> <%= edit_member_link(member) %></li>
        <li><%= member.phone %></li>
        <li><%= member.email %></li>
        <li><%= member.address %></li>
      </ul>
    </p>
  <%end%>